{
  "name": "@orrisai/agent-otp-telegram-bot",
  "version": "0.2.0",
  "description": "Telegram bot for Agent OTP user approval notifications",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@orrisai/agent-otp-shared": "workspace:*",
    "grammy": "^1.21.1",
    "hono": "^4.4.0",
    "@hono/node-server": "^1.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsup": "^8.1.0",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.9",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
