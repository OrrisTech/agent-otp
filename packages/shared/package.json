{
  "name": "@orrisai/agent-otp-shared",
  "version": "0.1.1",
  "description": "Shared types, schemas, and constants for Agent OTP",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/orristech/agent-otp.git",
    "directory": "packages/shared"
  },
  "homepage": "https://agentotp.com/docs",
  "license": "MIT",
  "author": "Agent OTP Team",
  "keywords": [
    "agent",
    "otp",
    "permission",
    "ai",
    "types"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
