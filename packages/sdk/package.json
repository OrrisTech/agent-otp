{
  "name": "@orrisai/agent-otp-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Agent OTP - One-Time Permission service for AI Agents",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orristech/agent-otp.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://agentotp.com/docs/sdk/typescript",
  "bugs": {
    "url": "https://github.com/orristech/agent-otp/issues"
  },
  "keywords": [
    "agent",
    "otp",
    "permission",
    "ai",
    "security",
    "authentication",
    "langchain",
    "ai-agent"
  ],
  "author": "Agent OTP Team",
  "license": "MIT",
  "dependencies": {
    "@orrisai/agent-otp-shared": "^0.2.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
